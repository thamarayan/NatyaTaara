<div class="container-fluid cf-dashboard">
    <div class="row">
        <div class="col-md-12 col-12" style="color: white;">
            <h2><span  style="background-color: rgb(208, 162, 242); color: black; padding: 2px 4px">SUBSCRIBERS LIST</span></h2>
            <p><a href="/backstage/viewWorkshop" style="text-decoration: none; color: yellow;">Back to Workshops</a></p>
            <p>Workshop : <span style="color: yellow;"><%= name %></span></p>
        </div>
        <div class="col-md-12 col-12">
            <% if (subscribers.length>0) { %>
                <table class="table table-responsive" style="color: white;">
                    <thead>
                      <tr>
                        <th scope="col">Name</th>
                        <th scope="col">Contact</th>
                        <th scope="col">Email</th>
                        <th scope="col">Payment</th>
                      </tr>
                    </thead>
                    <tbody>
    
                      <% subscribers.forEach(subscriber => { %>
                       <% if (subscriber.feesStatus) { %>

                        <tr>
                          <td><%= subscriber.name %></td>
                          <td><%= subscriber.contact %></td>
                          <td><%= subscriber.email %></td>
                          <% if (subscriber.feesStatus) { %>
                           <td>Paid</td>
                          <% } else { %>
                           <td>Not Yet</td>
                          <% } %>
                        </tr>
                        
                       <% } %>
              
                      <% }) %>
                    </tbody>
                </table>
            <% } else { %>
                <p style="color: rgb(208, 162, 242);">No Subscriptions yet</p>
            <% } %>
            
        </div>
    </div>
</div>